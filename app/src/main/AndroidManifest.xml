<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.exercisepertama">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Exercise Pertama"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExercisePertama">
        <activity android:name=".LihatData" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Daftar" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".DetailKontak" android:parentActivityName=".LihatData"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>